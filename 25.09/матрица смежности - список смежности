// перевод из матрицы смежности в список смежности

#include <iostream>
using namespace std;

int main() {

  int n;
  cin >> n;
  int**Msm = new int [n][n];
  for (int i = 0; i < n; i++){
      for (int j = 0; j < n; j++){
          cin >> Msm[i][j];
      }
  }
  for (int i = 0; i < n; i ++){
      cout << i << "{";
      for (int j = 0; j < n; j++){
          if (Msm[i][j] == 1){
              cout << j;
}
} cout << "}" << endl;
}

return 0;
}
